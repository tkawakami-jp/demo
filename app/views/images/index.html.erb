<% if !flash[:notice].blank? %>
  <div class="alert alert-info">
  <%= flash[:notice] %>
  </div>
<% end %>
<br />
<%= link_to "New image", new_image_path, class: "btn btn-primary" %>
<br />
<br />
<% @images.each do |image| %>
  <%= link_to image_tag(image.attachment_url) , image.attachment_url %>
  <%= button_to "Delete", image, method: :delete, class: "btn btn-danger", data: {confirm: "Are you sure?"} %>
  <br />
<% end %>